{
  "name": "@vue-toolkit/common",
  "version": "0.2.0",
  "description": "Completely style-able, fully accessible UI components, built with Vue.",
  "keywords": [
    "vue",
    "unstyled",
    "components"
  ],
  "author": {
    "name": "The Smyth Group",
    "email": "info@thesmythgroup.com",
    "url": "http://www.thesmythgroup.com/"
  },
  "license": "MIT",
  "homepage": "https://github.com/thesmythgroup/vue-toolkit",
  "repository": {
    "type": "git",
    "url": "https://github.com/thesmythgroup/vue-toolkit"
  },
  "bugs": {
    "url": "https://github.com/thesmythgroup/vue-toolkit/issues"
  },
  "files": [
    "dist/*",
    "sass/*"
  ],
  "main": "./dist/common.umd.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
